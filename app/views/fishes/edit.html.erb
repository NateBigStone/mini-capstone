<h1>Edit a Fish</h1>
<%= form_tag "/fishes/#{@fish.id}", method: :patch do %>
  <div>
  Name: <input type="text" name="name" value="<%=@fish.name %>">
  </div>
    <div>
  Price: <input type="number" name="price" value="<%=@fish.price %>">
  </div>
    <div>
  Image: <input type="text" name="image" value="<%=@fish.image %>">
  </div>
    <div>
  Description: <input type="text" name="description" value="<%=@fish.description %>">
  </div>
 <input type="submit" value="Update Fish">
    <%end%>


