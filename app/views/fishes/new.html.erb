<h1>New Fish!</h1>
<%= form_tag "/fishes", method: :post do %>
  <div>
    Name: <input type="text" name="name">
  </div>
  <div>
    Price: <input type="number" name="price">
  </div>
  <div>
    Image: <input type="text" name="image">
  </div>
  <div>
    Description: <input type="text" name="description">
  </div>
    <div>
    Supplier: <input type="integer" name="supplier_id">
  </div>
  <input type="submit" value="Create Fish">
<% end %>
